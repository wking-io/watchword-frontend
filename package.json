{
  "name": "watchword-frontend",
  "version": "0.1.0",
  "main": "src/main.js",
  "repository": "https://github.com/wking-io/watchword-frontend",
  "author": "William King <contactg@wking.io>",
  "license": "MIT",
  "scripts": {
    "start:dev": "babel-node tools/devServer.js",
    "build": "NODE_ENV=production babel-node tools/build.js",
    "build:prod":
      "NODE_ENV=production babel-node tools/build.js && yarn copy-redirects:prod",
    "build:dev": "NODE_ENV=development babel-node tools/build.js",
    "copy-redirects:prod": "cp _redirects dist/",
    "copy-redirects:dev": "cp _redirects build/",
    "postbuild": "babel-node tools/prodServer.js",
    "format": "elm-format --yes",
    "api": "graphqelm http://localhost:8000/graphql --output src/GraphQL"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chalk": "^2.1.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "elm-format": "^0.6.1-alpha",
    "elm-webpack-loader": "^4.4.0",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^1.1.5",
    "graphqelm": "3.1.12",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.1.1",
    "opn": "^5.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "url-loader": "^0.6.2",
    "webpack": "^3.5.6",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "elm": "0.18.0"
  }
}
